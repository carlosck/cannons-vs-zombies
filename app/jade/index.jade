doctype html
html
	head
		title= Game 
		link(href="style/normalize.css",rel='stylesheet')
		link(href="style/style.css",rel='stylesheet')
		meta(charset="UTF-8")
	body
		#full_container
			#stage
				#game_over
					p Game Over
					a#continue(href="#") Continue
				#world
					#yard
					#cannon_line
						#cannon_1.cannon
						#cannon_2.cannon
						#cannon_3.cannon
						#cannon_4.cannon
						#cannon_5.cannon
					#cannon_container
						#ball_1.ball_container(draggable="true")
							.ball.ball_1 50
						#ball_2.ball_container(draggable="true")
							.ball.ball_2 75
						#ball_3.ball_container(draggable="true")
							.ball.ball_3 150
					#score_container
						#score_title Score
						#score 0
							
					
					
		script(type='text/javascript',src="scripts/app.js")
		script(type='text/javascript',src="scripts/human.js")
		script(type='text/javascript',src="scripts/zombie_fast.js")
		script(type='text/javascript',src="scripts/zombie_heavy.js")
		script(type='text/javascript',src="scripts/neighbor.js")
		script(type='text/javascript',src="scripts/projectile.js")
		script(type='text/javascript',src="scripts/cannonball_fast.js")
		script(type='text/javascript',src="scripts/cannonball_medium.js")
		script(type='text/javascript',src="scripts/cannonball_heavy.js")
		
		script(type='text/javascript',src="scripts/fmw/core.js")
		script(type='text/javascript',src="scripts/fmw/animation.js")
